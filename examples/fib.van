end

defun removeChars(str, n) as
    local outStr = "";

    for(local i = n; i < len(str); i+=1) do
        outStr += str[i];
    end

    return outStr;
end

defun isFirst_And_Last_Same(list) as
    return list[0] == list[len(list) - 1];
end

