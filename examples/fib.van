defun join(list) as
    local outStr = "";

    for(local i = 0; i < len(list); i+=1) do
        outStr += list[i];
    end

    return outStr;
end

defun first_n_fib(n) as
    local x = 0;
    local y = 1;
    local z = 0;
    local out = [];

    for(local i = 1; i < n; i += 1) do
        out += x;
        z = x + y;
        x = y;
        y = z;
    end
    
    return out;
end

defun foo(x,y) as
