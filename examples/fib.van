defun fib(n) as
    local x = 0;
    local y = 1;
    local z = 0;

    for(local i = 1; i <= n; i += 1) do
        z = x + y;
        x = y;
        y = z;
    end

    return x;
end

defun foo(x,y) as
    local arr = [];

    for(local i = x; i <= y; i+=1) do
        arr += i;
    end

    return arr;
end

