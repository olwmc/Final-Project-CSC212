defun split(str, delim) as
    -- Temporary string and output array
    local tempStr = "";
    local outArr = [];

    -- Loop through the string
    for(local i = 0; i < len(str); i+=1) do

        -- If the index == the delimiter, 
        -- then add it to the array
        if(str[i] == delim) then
            outArr += tempStr;
            tempStr = "";
        
        else 
            -- Otherwhise, just add the index
            tempStr += str[i];
        end
    end

    -- Add the last string
    outArr += tempStr;

    -- Return the array
    return outArr;
end

-- A grocery list:
println(split("Black beans,Eggs,Milk,Bread,Chicken", ","));

